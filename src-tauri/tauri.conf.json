{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "desktop",
  "version": "0.1.0",
  "identifier": "1277",
  "build": {
    "beforeDevCommand": "cd ../../desktop && npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "cd ../../desktop && npm run build",
    "frontendDist": "../../desktop/dist"
  },
  "app": {
    "windows": [
      {
        "title": "任务大师 仪表盘",
        "width": 1280,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [
      "binaries/sidecar",
      "binaries/xhs-agent"
    ]
  }
}